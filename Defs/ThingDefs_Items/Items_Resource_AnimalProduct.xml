<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Honey -->
  <ThingDef ParentName="OrganicProductBase">
    <defName>InsectHoney</defName>
    <label>honey</label>
    <description>Honey made by Cubees. It is smooth, rich, and fulfills the recreation need of those who consume it. Because of its unique biological properties, it can nourish almost any creature and never rots.</description>
    <possessionCount>10</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/InsectHoney</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MarketValue>10</MarketValue>
      <Mass>0.025</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Nutrition>0.07</Nutrition>
      <FoodPoisonChanceFixedHuman>0.01</FoodPoisonChanceFixedHuman>
    </statBases>
    <!--<statOffsets>
      <Beauty>1</Beauty>
    </statOffsets>-->
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <preferability>MealFine</preferability>
      <joy>0.15</joy>
      <joyKind>Gluttonous</joyKind>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
      <canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
      <babiesCanIngest>true</babiesCanIngest>
    </ingestible>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
    <allowedArchonexusCount>100</allowedArchonexusCount>
  </ThingDef>

  <!-- Egg -->
  <ThingDef ParentName="EggFertBase">
    <defName>EggCubeeFertilized</defName>
    <label>Cubee egg (fert.)</label>
    <description>A fertilized cubee egg. If all goes well, it should hatch into a baby cubee. It can be eaten raw, but it's much better cooked.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/EggCubee</texPath>
      <drawSize>1</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>20.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>5</hatcherDaystoHatch>
        <hatcherPawn>Novacube_Cubee</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

</Defs>